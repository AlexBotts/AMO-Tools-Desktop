{
  "name": "amo-tools-desktop",
  "version": "0.1.5",
  "main": "main.js",
  "license": "MIT",
  "description": "AMO-Tools-Desktop",
  "author": "Advanced Manufacturing Office",
  "repository": "https://github.com/ORNL-AMO/AMO-Tools-Desktop.git",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "electron": "electron .",
    "lint": "ng lint",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "ng e2e",
    "pack": "build --dir",
    "dist": "build",
    "postinstall": "./node_modules/.bin/electron-rebuild",
    "windows": "./node_modules/.bin/build -w --x64 --ia32",
    "linux": "./node_modules/.bin/build -l --x64",
    "mac": "./node_modules/.bin/build -m --x64"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "amo-tools-suite": "^0.1.15",
    "angular2-color-picker": "^1.3.1",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "d3": "4.5.0",
    "d3-ng2-service": "^1.6.0",
    "dotenv": "^4.0.0",
    "electron-log": "^1.3.0",
    "electron-updater": "^1.8.2",
    "font-awesome": "^4.7.0",
    "ng2-bootstrap": "^1.3.1",
    "ngx-electron": "0.0.11",
    "rxjs": "^5.1.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.6"
  },
  "build": {
    "appId": "com.electron.amo-tools-desktop",
    "copyright": "Copyright 2017 ORNL. All rights reserved.",
    "productName": "AMO-Tools-Desktop",
    "directories": {
      "output": "../output/"
    },
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "installerHeaderIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "linux": {
      "target": [
        "tar.gz",
        "deb"
      ],
      "executableName": "AMO-Tools-Desktop",
      "maintainer": "Advanced Manufacturing Office"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    }
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.0",
    "@angular/compiler-cli": "^2.4.0",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.52",
    "@types/node": "~6.0.60",
    "chart.js": "^2.5.0",
    "codelyzer": "~2.0.0",
    "convert-units": "^2.0.1",
    "css-loader": "^0.26.2",
    "electron": "^1.4.15",
    "electron-builder": "^13.9.0",
    "electron-packager": "^8.5.2",
    "electron-rebuild": "^1.5.7",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng2-charts": "^1.5.0",
    "ngx-popover": "0.0.16",
    "node-loader": "^0.6.0",
    "protractor": "~5.1.0",
    "style-loader": "^0.13.2",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "typescript": "~2.2.1"
  }
}
