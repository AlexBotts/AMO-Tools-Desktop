{
  "name": "amo-tools-desktop",
  "version": "0.4.0-beta",
  "main": "main.js",
  "license": "MIT",
  "description": "MEASUR",
  "author": "Advanced Manufacturing Office",
  "repository": "https://github.com/ORNL-AMO/AMO-Tools-Desktop.git",
  "angular-cli": {},
  "engines": {
    "node": "^8.11.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-aot": "ng build --aot",
    "build-watch": "ng build --watch",
    "build-prod": "ng build --aot",
    "build-prod-windows": "./scripts/ng.sh build --prod --aot",
    "clean": "rm -rf node_modules && rm -rf dist && rm -f package-lock.json",
    "electron": "electron .",
    "lint": "ng lint",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "postinstall": "electron-builder install-app-deps",
    "e2e": "ng e2e",
    "pack": "build --dir",
    "dist": "build",
    "windows": "./node_modules/.bin/build -w --x64 --ia32",
    "linux": "./node_modules/.bin/build -l --x64",
    "mac": "./node_modules/.bin/build -m --x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.8",
    "@angular/common": "7.2.8",
    "@angular/compiler": "7.2.8",
    "@angular/compiler-cli": "7.2.8",
    "@angular/core": "7.2.8",
    "@angular/forms": "7.2.8",
    "@angular/http": "7.2.8",
    "@angular/platform-browser": "7.2.8",
    "@angular/platform-browser-dynamic": "7.2.8",
    "@angular/platform-server": "7.2.8",
    "@angular/router": "7.2.8",
    "ajv": "^5.2.2",
    "amo-tools-suite": "file:../AMO-Tools-Suite",
    "bootstrap": "4.3.1",
    "c3": "^0.4.18",
    "core-js": "^2.4.1",
    "d3": "4.5.0",
    "electron-log": "3.0.1",
    "electron-updater": "4.0.6",
    "font-awesome": "^4.7.0",
    "ngx-electron": "2.1.1",
    "regression": "^2.0.0",
    "rxjs": "6.4.0",
    "rxjs-compat": "^6.4.0",
    "save-svg-as-png": "1.2.0",
    "zone.js": "^0.8.12"
  },
  "build": {
    "appId": "com.electron.amo-tools-desktop",
    "copyright": "Copyright 2017 ORNL. All rights reserved.",
    "productName": "MEASUR",
    "directories": {
      "output": "../output/"
    },
    "win": {
      "target": "nsis",
      "icon": "src/assets/icons/win/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "installerHeaderIcon": "../AMO-Tools-Desktop/src/favicon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "linux": {
      "target": [
        "tar.gz",
        "deb"
      ],
      "icon": "src/assets/icons/png/",
      "executableName": "MEASUR",
      "maintainer": "Advanced Manufacturing Office"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "src/assets/icons/mac/icon.icns"
    }
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.5",
    "@angular-devkit/core": "7.3.5",
    "@angular-devkit/schematics": "7.3.5",
    "@angular/cli": "^7.3.5",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "4.14.100",
    "@types/node": "11.11.1",
    "chart.js": "^2.7.0",
    "codelyzer": "4.5.0",
    "electron": "^1.7.6",
    "electron-builder": "19.53.2",
    "electron-download-tf": "^4.3.4",
    "electron-packager": "13.1.1",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "json2csv": "^4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lodash.foreach": "^4.5.0",
    "lodash.keys": "^4.2.0",
    "moment": "^2.18.1",
    "ngx-bootstrap": "3.2.0",
    "ngx-clipboard": "^11.1.3",
    "protractor": "^5.4.1",
    "ts-node": "8.0.3",
    "tslint": "^5.13.1",
    "typescript": "3.2.4"
  }
}
